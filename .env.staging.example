# NeuroVault Staging Environment
# Copy this to .env.staging and fill in your values

# Network & RPC
VITE_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
VITE_TARGET_CHAIN_ID=11155111

# Smart Contract
VITE_MEMORY_REGISTRY_ADDRESS=0x...

# IPFS
VITE_IPFS_API_URL=https://ipfs.infura.io:5001/api/v0
VITE_IPFS_GATEWAY=https://dweb.link/ipfs
VITE_WEB3STORAGE_KEY=eyJ...

# Block Explorer
VITE_BLOCK_EXPLORER_BASE=https://sepolia.etherscan.io

# Monitoring (Optional)
SENTRY_DSN=

# Feature Flags
VITE_ENABLE_ENS=true
VITE_ENABLE_TRANSACTION_UI=true
