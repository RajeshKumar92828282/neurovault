      "scripts": {
            "dev": "vite",
            "build": "vite build",
            "preview": "vite preview",
            "deploy": "gh-pages -d dist",
            "test": "vitest",
            "test:ui": "vitest --ui",
            "test:run": "vitest run",
            "type-check": "tsc --noEmit",
            "lint": "tsc --noEmit && echo 'Type checking passed'",
            "build:wasm": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Push-Location './contracts/stylus/memory_registry'; ./build-memory-registry.ps1; Pop-Location\"",
            "copy:wasm": "node ./tools/copy-wasm.js",
            "build:wasm:copy": "npm run build:wasm && npm run copy:wasm",
            "merge:deployments": "ts-node scripts/merge-deployments.ts",
            "postbuild": "node ./scripts/postbuild-copy.js",
            "fetch:wasm": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$u = $env:WASM_URL; if(-not $u){ $u = $args[0] }; if(-not $u){ Write-Error 'Provide WASM_URL environment variable or first argument'; exit 1 }; New-Item -ItemType Directory -Force -Path 'public/stylus' | Out-Null; Invoke-WebRequest -Uri $u -OutFile 'public/stylus/memory_registry.wasm' -UseBasicParsing\"",
            "frontend:fetch:wasm": "node ./scripts/fetch_wasm.js --url $npm_config_url --out ./frontend/public/wasm/stylus_wasm.wasm --sha $npm_config_sha",
            "verify:wasm": "node ./scripts/verify_wasm_fetch.js",
            "verify:wasm:local": "node ./scripts/verify_wasm_fetch.js --local ./contracts/stylus/memory_registry/target/wasm32-unknown-unknown/release/memory_registry.wasm",
            "verify:wasm:remote": "node ./scripts/verify_wasm_fetch.js --url $npm_config_url --sha $npm_config_sha --out ./memory_registry.wasm",
            "ping:stylus": "node ./scripts/call_stylus_ping.js",
            "dev:node": "npx hardhat node",
            "dev:deploy": "npx hardhat run scripts/deploy.ts --network localhost",
            "deploy:arb": "npx hardhat run scripts/deploy.ts --network arbitrumSepolia",
            "verify:arb": "npx hardhat run scripts/verify.ts --network arbitrumSepolia"
      },
      "vitest": {
            "test": {
                  "exclude": [
                        "tests/MemoryRegistryV2.test.ts",
                        "tests/useWallet.test.ts"
                  ]
            }
      }
}
