# NeuroVault Production Environment
# Copy this to .env.production and fill in your values

# Network & RPC (Arbitrum One mainnet)
VITE_RPC_URL=https://arbitrum-mainnet.infura.io/v3/YOUR_INFURA_KEY
VITE_TARGET_CHAIN_ID=42161

# Smart Contract
VITE_MEMORY_REGISTRY_ADDRESS=0x...

# IPFS
VITE_IPFS_API_URL=https://ipfs.infura.io:5001/api/v0
VITE_IPFS_GATEWAY=https://dweb.link/ipfs
VITE_WEB3STORAGE_KEY=eyJ...

# Block Explorer
VITE_BLOCK_EXPLORER_BASE=https://arbiscan.io

# Monitoring (Recommended)
SENTRY_DSN=https://...@sentry.io/...

# Feature Flags
VITE_ENABLE_ENS=true
VITE_ENABLE_TRANSACTION_UI=true
